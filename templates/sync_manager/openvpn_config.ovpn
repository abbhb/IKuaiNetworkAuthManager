client
dev-type tun
dev tunx
tun-mtu 1400
cipher BF-CBC
comp-lzo
proto {{ protocol }}
remote {{ server_host }} {{ server_port }}
resolv-retry infinite
nobind
persist-key
persist-tun
verb 3
auth-user-pass
script-security 2
client-cert-not-required

<ca>
{{ ca_cert }}

</ca>

# redirect-gateway def1 bypass-dns  # uncomment to set as default gateway
# route-nopull  # uncomment to disable server route push
#
